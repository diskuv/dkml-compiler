            "cwd": "${workspaceFolder}/_build/prefix/src-ocaml/stdlib",
        },
        {
            "type": "midas-rr",
            "request": "attach",
            "name": "Launch replay debug session",
            "cwd": "${workspaceFolder}",
            "trace": "Off",
            "gdbPath": "/usr/bin/gdb",
            "rrPath": "/usr/bin/rr",
            "setupCommands": [
                "set sysroot /",
